{
  "name": "temporary-web-hosting-server",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --copy-files --extensions \".ts,.tsx\"",
    "dev": "NODE_ENV=test nodemon -r ./src/test/utils/babel.js src/** --exec babel-node --extensions \".ts,.tsx\" src/index.ts",
    "test": "NODE_ENV=test mocha -t 10000 -r ./src/test/utils/babel.js ./src/test/**/*.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "type-check": "tsc --noEmit **/*.ts --esModuleInterop"
  },
  "dependencies": {
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "redis": "^3.0.2",
    "redis-mock": "^0.49.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/redis": "^2.8.18",
    "@types/redis-mock": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "superagent": "^5.2.2",
    "typescript": "^3.8.3"
  }
}
